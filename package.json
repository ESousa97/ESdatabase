{
  "name": "next-material-kit-local",
  "version": "1.2.2",
  "description": "NextJS Version of Material Kit React - Local Version",
  "main": "index.js",
  "scripts": {
    "dev": "next dev -p 3001",
    "build": "next build",
    "start": "next start -p 3001",
    "lint": "next lint --dir .",
    "lint:fix": "next lint --fix --dir .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest",
    "test:ci": "jest --ci --coverage",
    "test:watch": "jest --watch",
    "clean": "rimraf .next out dist coverage",
    "clean:all": "rimraf node_modules .next out dist coverage",
    "install:clean": "npm run clean:all && rimraf package-lock.json && npm install && npm run dev",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/creativetimofficial/nextjs-material-kit.git"
  },
  "keywords": [
    "next",
    "nextjs",
    "react",
    "reactjs",
    "material",
    "material-kit"
  ],
  "author": "Creative Tim",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/creativetimofficial/nextjs-material-kit/issues"
  },
  "homepage": "https://demos.creative-tim.com/nextjs-material-kit",
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^5.18.0",
    "@mui/material": "^7.3.6",
    "axios": "^1.12.0",
    "classnames": "2.5.1",
    "date-fns": "^3.6.0",
    "dompurify": "^3.3.1",
    "moment": "2.30.1",
    "next": "^14.2.35",
    "next-auth": "^4.24.12",
    "nouislider": "15.8.1",
    "pg": "^8.11.3",
    "prop-types": "15.8.1",
    "react": "^18.2.0",
    "react-click-outside": "^3.0.1",
    "react-datetime": "3.3.1",
    "react-dom": "^18.2.0",
    "react-markdown": "^9.0.1",
    "react-router-dom": "^6.22.3",
    "react-slick": "0.31.0",
    "react-spring": "^9.7.3",
    "react-toastify": "^10.0.5",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.0",
    "sass": "1.97.1",
    "styled-components": "5.3.5",
    "webpack": "^5.104.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.0",
    "@commitlint/config-conventional": "^19.6.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/node": "^25.0.3",
    "@types/react": "^18.2.74",
    "eslint": "^8.57.1",
    "eslint-config-next": "^15.1.0",
    "eslint-config-prettier": "^10.0.1",
    "framer-motion": "^11.0.24",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.10",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "typescript": "^5.9.3"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,json,md,yml,yaml,css,scss}": [
      "prettier --write"
    ],
    "**/*.{js,jsx,ts,tsx}": [
      "next lint --fix --dir . --file"
    ]
  }
}
